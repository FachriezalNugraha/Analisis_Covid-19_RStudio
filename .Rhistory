library(httr)
resp_yogya <- GET("https://data.covid19.go.id/public/api/prov_detail_DAERAH_ISTIMEWA_YOGYAKARTA.json")
cov_yogya_raw <- content(resp_yogya, as = "parsed", simplifyVector = TRUE)
names(cov_yogya_raw)
cov_yogya_raw$kasus_total
cov_yogya_raw$meninggal_persen
cov_yogya_raw$sembuh_persen
cov_yogya <- cov_yogya_raw$list_perkembangan
str(cov_yogya)
head(cov_yogya)
library(dplyr)
new_cov_yogya <-
cov_yogya %>%
select(-contains("DIRAWAT_OR_ISOLASI")) %>%
select(-starts_with("AKUMULASI")) %>%
rename(
kasus_baru = KASUS,
meninggal = MENINGGAL,
sembuh = SEMBUH
) %>%
mutate(
tanggal = as.POSIXct(tanggal / 1000, origin = "1970-01-01"),
tanggal = as.Date(tanggal)
)
str(new_cov_yogya)
View(new_cov_yogya)
View(cov_yogya)
library(ggplot2)
library(hrbrthemes)
ggplot(new_cov_yogya, aes(x = tanggal, y = kasus_baru)) +
geom_col()
